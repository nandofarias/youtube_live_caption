<section>
  <form phx-submit="post">
    <input type="text" name="url" value="<%= @url %>" placeholder="Place your url caption here" autocomplete="off"/>
    <input type="text" name="text" value="<%= @text %>" placeholder="Translate here" autocomplete="off"/>
    <button type="submit" phx-disable-with="Posting...">Post</button>
  </form>
</section>
